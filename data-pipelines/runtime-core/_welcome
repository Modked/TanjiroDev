import {WAMessageStubType} from '@whiskeysockets/baileys'
import fetch from 'node-fetch'

export async function before(m, {conn, participants, groupMetadata}) {
  if (!m.messageStubType || !m.isGroup) return !0;
    let pp = await conn.profilePictureUrl(m.messageStubParameters[0], 'image').catch(_ => welcome)
    let pp2 = await conn.profilePictureUrl(m.messageStubParameters[0], 'image').catch(_ => adios)
  let img = await (await fetch(`${pp}`)).buffer()
  let img2 = await (await fetch(`${pp2}`)).buffer()

  let chat = global.db.data.chats[m.chat]

  if (chat.welcome && m.messageStubType == 27) {
    let wel = ` â•’â•¾âŸ¢ ğ‘ºğ´ğ‘ºğ‘ˆğ¾ğ¸-ğµğ›©ğ‘‡ ğŸ² \n â”ƒã€ _*Ù†Ù€Ù€Ù€Ù€Ù€ÙˆØ±Øª ğŸ‘‹*_ ğŸ‘‹ ã€\n â””â”¬âŸ¢ ã€ @${m.messageStubParameters[0].split`@`[0]} ã€\n   â”ƒ ${groupMetadata.subject}\n   â”ƒğŸ‰  *Ù‚Ù€Ù†Ù€Ø§Ø© Ø§Ù„Ø£Ù†Ù€Ù…Ù€ÙŠ* \n  â”ƒğŸŒŸ *ØªÙ€Ù†Ù€ÙˆØ±Ù†Ù€Ø§ Ø£ÙƒÙ€Ø«Ù€Ø±* https://whatsapp.com/channel/0029VaklBGFHFxOwODjsoP13\n  â”—â”â”â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”â”â”›`
await conn.sendMini(m.chat, packname, dev, wel, img, img, channel, fkontak)
  }

  if (chat.welcome && m.messageStubType == 28) {
   let bye = ` â•’â•¾âŸ¢ ğ‘ºğ´ğ‘ºğ‘ˆğ¾ğ¸-ğµğ›©ğ‘‡ â˜ï¸ \n â”ƒã€ *Ø§Ù„Ù‰ Ø§Ù„Ù„Ù‚Ø§Ø¡* ğŸ–ï¸ ã€\n â””â”¬âŸ¢ ã€ @${m.messageStubParameters[0].split`@`[0]} ã€\n   â”ƒğŸ² *Ù‡Ø°Ù‡ Ù‡Ùˆ Ø§Ù„Ø£Ù…Ø±ğŸ‘ŒğŸ»*\n   â”ƒğŸ‰ *ØªÙˆØ³Ù„ Ø§Ù„Ù…Ø·ÙˆØ± ØªØ§Ù†Ø¬ÙŠØ±Ùˆ Ù„ÙŠØ±Ø¬Ø¹Ùƒ*\n   â”ƒğŸ‰  *Ù‚Ù†Ø§Ø© Ø§Ù„Ø£Ù†Ù…ÙŠ* \n   â”ƒğŸŒŸ *Ø£Ø¹Ù…Ù„ Ù…ØªØ§Ø¨Ø¹Ø© ÙˆØ§Ù†Ù‚Ù„Ø¹ ğŸ™‚* https://whatsapp.com/channel/0029VaklBGFHFxOwODjsoP13\n   â”—â”â”â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”â”â”›`
await conn.sendMini(m.chat, packname, dev, bye, img2, img2, channel, fkontak)
  }

  if (chat.welcome && m.messageStubType == 32) {
    let kick = ` â•’â•¾âŸ¢ ğ‘ºğ´ğ‘ºğ‘ˆğ¾ğ¸-ğµğ›©ğ‘‡ â˜ï¸\n â”ƒã€ *Ø§Ù„Ù‰ Ø§Ù„Ù„Ù‚Ø§Ø¡* ğŸ–ï¸ ã€\n â””â”¬âŸ¢ ã€ @${m.messageStubParameters[0].split`@`[0]} ã€\n   â”ƒğŸ² *Ù‡Ø°Ù‡ Ù‡Ùˆ Ø§Ù„Ø£Ù…Ø±ğŸ‘ŒğŸ»*\n   â”ƒğŸ‰ *ØªÙˆØ³Ù„ Ø§Ù„Ù…Ø·ÙˆØ± ØªØ§Ù†Ø¬ÙŠØ±Ùˆ Ù„ÙŠØ±Ø¬Ø¹Ùƒ*\n   â”ƒğŸ‰  *Ù‚Ù†Ø§Ø© Ø§Ù„Ø£Ù†Ù…ÙŠ*\n   â”ƒğŸŒŸ *ØªÙ€Ù†Ù€ÙˆØ±Ù†Ù€Ø§* https://whatsapp.com/channel/0029VaklBGFHFxOwODjsoP13\n   â”—â”â”â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”â”â”›`
await conn.sendMini(m.chat, packname, dev, kick, img2, img2, channel, fkontak)
}}

